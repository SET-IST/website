{
  "name": "set24-website",
  "version": "0.1.6",
  "private": true,
  "prisma": {
    "schema": "database/prisma/schema.prisma"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "dev:db:start": "cd database && docker compose up -d",
    "dev:db:stop": "cd database && docker compose rm -s -f -v",
    "dev:db:push": "yarn prisma db push",
    "dev:db:seed": "command -v psql >/dev/null && psql -h localhost -p 5432 -U postgres -c 'create database setdb' && psql -h localhost -p 5432 -U postgres setdb < ./database/seeds/PROD_db_seed-24-02-2024.sql",
    "dev:db:dump": "command -v psql >/dev/null && pg_dump -h localhost -p 5432 -U postgres setdb -f ./database/seeds/db_seed-$(date +'%d-%m-%Y').sql && echo \"Dump saved to ./database/seeds/db_seed-$(date +'%d-%m-%Y').sql\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@aws-sdk/client-s3": "^3.689.0",
    "@aws-sdk/s3-request-presigner": "^3.689.0",
    "@babel/core": "^7.26.0",
    "@edgestore/react": "^0.1.7",
    "@edgestore/server": "^0.1.7",
    "@mantine/carousel": "^7.14.0",
    "@mantine/core": "^7.14.0",
    "@mantine/dropzone": "^7.14.0",
    "@mantine/form": "^7.14.0",
    "@mantine/hooks": "^7.14.0",
    "@mantine/notifications": "^7.14.0",
    "@prisma/client": "^5.4.2",
    "@tabler/icons-react": "^2.42.0",
    "@tanstack/react-query": "^4.36.1",
    "@types/node": "^18.19.64",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "classnames": "^2.5.1",
    "cookies": "^0.8.0",
    "embla-carousel-autoplay": "^8.3.1",
    "embla-carousel-react": "^8.3.1",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.17",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "mantine-form-yup-resolver": "^2.0.0",
    "mime": "^3.0.0",
    "mobile-detect": "^1.4.5",
    "next": "13.2.4",
    "next-api-decorators": "^2.0.2",
    "next-auth": "^4.24.10",
    "path-to-regexp": "^6.2.1",
    "qr-scanner": "^1.4.2",
    "react": "18.2.0",
    "react-dom": "18.3.1",
    "react-icons": "^4.12.0",
    "react-qr-code": "^2.0.15",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-qrcode-logo": "^2.10.0",
    "react-transition-group": "^4.4.5",
    "sharp": "^0.33.5",
    "signale": "^1.4.0",
    "tailwindcss": "^3.4.14",
    "typescript": "4.9.5",
    "yup": "^1.4.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query-devtools": "^4.36.1",
