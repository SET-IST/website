{
  "name": "set24-website",
  "version": "0.1.2",
  "private": true,
  "prisma": {
    "schema": "database/prisma/schema.prisma"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "dev:db:start": "cd database && docker compose up -d",
    "dev:db:stop": "cd database && docker compose rm -s -f -v",
    "dev:db:push": "yarn prisma db push",
    "dev:db:seed": "command -v psql >/dev/null && psql -h localhost -p 5432 -U postgres -c 'create database setdb' && psql -h localhost -p 5432 -U postgres setdb < ./database/seeds/db_seed-13-03-2023.sql || echo 'Error: You need to have postgresql 15.1 installed'",
    "dev:db:dump": "command -v psql >/dev/null && pg_dump -h localhost -p 5432 -U postgres setdb -f ./database/seeds/db_seed-$(date +'%d-%m-%Y').sql && echo \"Dump saved to ./database/seeds/db_seed-$(date +'%d-%m-%Y').sql\" || echo 'Error: You need to have postgresql 15.1 installed'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.304.0",
    "@babel/core": "^7.21.0",
    "@mantine/core": "^7.0.1",
    "@mantine/hooks": "^7.0.1",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.9.0",
    "@tabler/icons-react": "^2.35.0",
    "@tanstack/react-query": "^4.24.9",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.11.19",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-qr-reader": "^2.1.4",
    "@yudiel/react-qr-scanner": "^1.1.10",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "cookies": "^0.8.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.1.6",
    "formik": "^2.2.9",
    "ioredis": "^5.3.0",
    "luxon": "^3.2.1",
    "mime": "^3.0.0",
    "mobile-detect": "^1.4.5",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "next": "13.2.4",
    "next-api-decorators": "^2.0.0",
    "next-auth": "^4.19.2",
    "path-to-regexp": "^6.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-qr-code": "^2.0.11",
    "react-query": "^3.39.2",
    "react-transition-group": "^4.4.5",
    "sharp": "^0.31.3",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "4.9.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query-devtools": "^4.24.14",
    "@types/bcrypt": "^5.0.0",
    "@types/cookies": "^0.7.7",
    "@types/lodash": "^4.14.199",
    "@types/luxon": "^3.2.0",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/react-select": "^5.0.1",
    "@types/react-transition-group": "^4.4.5",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.2",
    "postcss": "^8.4.30",
    "postcss-preset-mantine": "^1.7.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.4",
    "prisma": "^4.9.0"
  },
  "browser": {
    "fs": false
  }
}
