    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.20",
    "husky": "^8.0.3",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.8",
    "prisma": "^5.8.1"
  },
