    "@types/signale": "^1.4.7",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.2",
    "postcss": "^8.4.36",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.4",
    "prisma": "^6.18.0"
  },
