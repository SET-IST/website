{
  "name": "jet26-website",
  "version": "0.2.0",
  "private": true,
  "prisma": {
    "schema": "database/prisma/schema.prisma"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "dev:db:start": "cd database && docker compose up -d",
    "dev:db:stop": "cd database && docker compose rm -s -f -v",
    "dev:db:seed": "psql -h localhost -p 5432 -U postgres -c 'create database setdb' && psql -h localhost -p 5432 -U postgres setdb < ./database/seeds/PROD_db_seed-25-10-2025.sql && yarn dev:db:seed:resolve",
    "dev:db:seed:resolve": "prisma migrate resolve --applied 00000000000000_baseline && prisma migrate deploy",
    "dev:db:dump": "pg_dump -h localhost -p 5432 -U postgres setdb -f ./database/seeds/db_seed-$(date +'%d-%m-%Y').sql && echo \"Dump saved to ./database/seeds/db_seed-$(date +'%d-%m-%Y').sql\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@aws-sdk/client-s3": "^3.496.0",
    "@aws-sdk/s3-request-presigner": "^3.496.0",
    "@babel/core": "^7.21.0",
    "@edgestore/react": "^0.1.6",
    "@edgestore/server": "^0.1.6",
    "@mantine/carousel": "^8.3.5",
    "@mantine/core": "^8.3.5",
    "@mantine/dropzone": "^8.3.5",
    "@mantine/form": "^8.3.5",
    "@mantine/hooks": "^8.3.5",
    "@mantine/notifications": "^8.3.5",
    "@prisma/client": "^6.18.0",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.5",
    "@yudiel/react-qr-scanner": "^2.4.1",
    "axios": "^1.12.0",
    "bcrypt": "^5.1.0",
    "better-auth": "^1.3.32",
    "caniuse-lite": "^1.0.30001700",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "cookies": "^0.8.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "mantine-form-yup-resolver": "^2.0.0",
    "mime": "^3.0.0",
    "mobile-detect": "^1.4.5",
    "next": "16.0.9",
    "next-api-decorators": "^2.0.0",
    "path-to-regexp": "^6.2.1",
    "qr-scanner": "^1.4.2",
    "react": "19.2.0",
    "react-custom-roulette-r19": "^2.0.3",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-qrcode-logo": "^4.0.0",
    "react-transition-group": "^4.4.5",
    "sharp": "^0.33.2",
    "signale": "^1.4.0",
    "tailwindcss": "^3.3.3",
    "typescript": "v5",
    "yup": "^1.3.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/bcrypt": "^5.0.0",
    "@types/caniuse-lite": "^1",
    "@types/cookies": "^0.7.7",
