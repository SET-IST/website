  "rules": {
    "import/no-restricted-paths": [
      "error",
      {
        "zones": [
          {
            "target": "./core",
            "from": "./lib"
          },
          {
            "target": "./core",
            "from": "./pages"
          },
          {
            "target": "./lib",
